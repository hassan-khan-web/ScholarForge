{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/frontend/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/index.py"
        }
    ]
}